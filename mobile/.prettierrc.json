{
  "endOfLine": "auto",
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "all",
  "importOrder": [
    "^(react|react-native)$",
    "^(expo(/|-)(.*)$)|^(expo$)",
    "<THIRD_PARTY_MODULES>",
    "^@(components|public|hooks|config|fonts|utils|core|screens|graphql|lib|helpers|generated)/(.*)$",
    "^./.+$",
    "^../(?!(../+)).+$",
    "^../../(?!(../+)).+$",
    "^../../../(?!(../+)).+$",
    "^../../../../(?!(../+)).+$",
    "^../../../../../(?!(../+)).+$"
  ]
}