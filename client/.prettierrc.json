{
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "all",
  "importOrder": [
    "^react$",
    "^(next/(.*)$)|^(next$)",
    "<THIRD_PARTY_MODULES>",
    "^@(components|public|styles|hooks|config|utils|generated|graphql|lib|helpers)/(.*)$",
    "^./(?!((.*).module.css)).+$",
    "^../(?!(../|(.*).module.css)+).+$",
    "^../../(?!(../|(.*).module.css)+).+$",
    "^../../../(?!(../|(.*).module.css)+).+$",
    "^../../../../(?!(../|(.*).module.css)+).+$",
    "^../../../../../(?!(../|(.*).module.css)+).+$",
    "(.*).module.css"
  ]
}